    "# Break and Continue Statement\n",
    "\n",
    "sum = 0\n",
    "for i in range(20):\n",
    "    if i % 2 == 0:\n",
    "        continue\n",
    "    \n",
    "    if i == 5:\n",
    "        break\n",
    "    \n",
    "    sum += i\n",
    "\n",
    "print('Sum:', sum) # Sum: 4"
   ]
  }
