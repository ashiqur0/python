    "# \n",
    "t = int(input())\n",
    "\n",
    "for i in range(t):\n",
    "    number = int(input())\n",
    "    \n",
    "    if number == 0:\n",
    "        print(0)\n",
    "        continue\n",
    "    \n",
    "    while number > 0:\n",
    "        print(number % 10, end=\" \")\n",
    "        number //= 10\n",
    "    print()"
